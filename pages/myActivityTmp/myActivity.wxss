.myActivityTmp_t{
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 30rpx;
  border-bottom: 1rpx solid #eee;
  width: 90%;
  height: 80rpx;
  margin: 0 auto;
}
.tag{
  color: #FF6029;
}
.myActivityTmp_m{
  display: flex;
  align-items: center;
  justify-content: flex-start;
  width: 90%;
  margin: 10rpx auto;
}
.myActivityTmp_m image{
  width: 150rpx;
  height: 150rpx;
}
.detail{
  display: flex;
  flex-direction: column;
  align-items: flex-start;
  margin-left: 30rpx;
}
.detail_time{
  color: #999;
  font-size: 25rpx;
  margin-top: 15rpx;
}
.detail_address{
  color: #FF6029;
  font-size: 30rpx;
  display: flex;
  align-items: center;
  margin-top: 25rpx;
}
.detail_address image{
  width: 50rpx;
  height: 50rpx;
}
.num{
  display: flex;
  justify-content: flex-end;
  align-items: center;
  border-bottom: 1rpx solid #eee;
  width: 90%;
  height: 80rpx;
  margin: 0 auto;
  color: #999;
  margin-top: 30rpx;
  padding-bottom: 20rpx;
}
.cancle{
  width: 100%;
  height: 100rpx;
  border-bottom: 10rpx solid #eee;
  display: flex;
  justify-content: flex-end;
  align-items: center;
}
.cancle .cancle_btn{
  width: 150rpx;
  height: 50rpx;
  line-height: 50rpx;
  display: inline-block;
  vertical-align: middle;
  text-align: center;
  border-radius: 50rpx;
  border: 1rpx solid #FF6029;
  color: #FF6029;
  background: rgb(243, 199, 185);
  font-size: 30rpx;
  margin-bottom: 30rpx;
}


